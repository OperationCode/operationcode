<ul id="about" class="dropdown-content">
  <li><%= link_to 'History', history_path %></li>
  <li><%= link_to 'About Operation Code', about_path %></li>
  <li><%= link_to 'FAQ', faq_path %></li>
  <li class="divider"></li>
  <li><%= link_to 'Press', press_path %></li>
  <li><%= link_to 'Staff & Board', board_path %></li>
  <li><%= link_to 'Sponsors', sponsors_path %></li>
  <li role="separator" class="divider"></li>
  <li><%= link_to 'TAP Briefing', "http://operationcode.github.io/tap-deck" %></li>
</ul>

<ul id="programs-and-services" class="dropdown-content">
  <li><%= link_to 'Software Mentorship', mentorship_path %></li>
  <li><%= link_to 'Scholarships', scholarships_path %></li>
  <li><%= link_to 'Empoyer Services', employers_path %></li>
  <li><%= link_to 'Deploy', deploy_path %></li>
  <li role="separator" class="divider"></li>
  <li><%= link_to 'Code Schools', codeschools_path %></li>
</ul>

<div class="navbar-fixed">
  <nav class="white" role="navigation">
    <div class="nav-wrapper container">
      <%= link_to image_tag('operationcode_logo.svg'), root_path, class: 'brand-logo'%>

      <ul class="right hide-on-med-and-down">
        <li class='grey-text'><a class="dropdown-button" href="#!" data-constrainwidth="false" data-beloworigin="true" data-activates="about">About<i class="material-icons right">arrow_drop_down</i></a></li>
        <li><a class="dropdown-button" href="#!" data-constrainwidth="false" data-beloworigin="true" data-activates="programs-and-services">Programs And Services<i class="material-icons right">arrow_drop_down</i></a></li>
        <li><%= link_to 'Blog', blog_path %></li>
        <li><%= link_to 'Donate', donate_path %></li>
        <% if veteran_signed_in? %>
          <li><%= link_to 'Profile', profile_path %></li>
          <li><%= link_to 'Sign Out', destroy_veteran_session_path, method: :delete %></li>
        <% else %>
          <li><%= link_to 'Join', new_veteran_registration_path %></li>
        <% end %>
      </ul>

      <ul id="nav-mobile" class="side-nav">
        <li><a href="#">Navbar Link</a></li>
      </ul>
      <a href="#" data-activates="nav-mobile" class="button-collapse"><i class="material-icons">menu</i></a>
    </div>
  </nav>
</div>
