<div class="container">
  <div class="row">
    <%= render partial: 'profile/tabs' %>
  </div>

  <div class="row">
    <div class="col m5" role="main">

      <%= card(title: 'Account Details', icon: { name: :settings, path: profile_edit_path } ) do %>
        <%=  render partial: 'profile/home/details' %>
      <% end %>
    </div>

    <div class="col m5" role="main">
      <%= card(title: :Feed) do %> 
        <%=  render partial: 'profile/home/announcements' %>
      <% end %>
    </div>
  </div>

  <% unless current_veteran.mentor? %>
  <div class="row">
    <div class="col m5" role="main">
      <%= card(title: 'Mentor Details') do %>
        <%=  render partial: 'profile/home/mentor' %>
      <% end %>
    </div>
  </div>
  <% end %>

</div>

